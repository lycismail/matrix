  <remap from="/sensor/matrix/image3/compressed" to="/sensor/matrix1/image3/compressed"/>
  <remap from="/sensor/matrix/image4/compressed" to="/sensor/matrix1/image4/compressed"/>
  <remap from="/sensor/matrix/image1" to="/sensor/matrix1/image1"/>
  <remap from="/sensor/matrix/image2" to="/sensor/matrix1/image2"/>
  <remap from="/sensor/matrix/image3" to="/sensor/matrix1/image3"/>
  <remap from="/sensor/matrix/image4" to="/sensor/matrix1/image4"/>
  <remap from="/sensor/matrix/lines1" to="/sensor/matrix1/lines1"/>
  <remap from="/sensor/matrix/lines2" to="/sensor/matrix1/lines2"/>
  <remap from="/sensor/matrix/lines3" to="/sensor/matrix1/lines3"/>
  <remap from="/sensor/matrix/lines4" to="/sensor/matrix1/lines4"/>
  <remap from="/sensor/matrix/signals1" to="/sensor/matrix1/signals1"/>
  <remap from="/sensor/matrix/signals2" to="/sensor/matrix1/signals2"/>
  <remap from="/sensor/matrix/signals3" to="/sensor/matrix1/signals3"/>
  <remap from="/sensor/matrix/signals4" to="/sensor/matrix1/signals4"/>
  <rosparam>
    /matrix1/ip: 192.168.4.10:5560
  </rosparam>
</node>

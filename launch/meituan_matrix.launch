<launch>

<node name="matrix1" pkg="matrix_driver" type="matrix_driver" output="screen">
  <remap from="/matrix/ip" to="/matrix1/ip"/>

  <remap from="/sensor/matrix/freespace1" to="/sensor/matrix1/freespace1"/>
  <remap from="/sensor/matrix/freespace2" to="/sensor/matrix1/freespace2"/>
